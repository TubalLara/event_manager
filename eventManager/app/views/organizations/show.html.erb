<div class="row">
  <div class="col-xs-4">
    <div class="well">
      <strong>Name:</strong>
      <%= @organization.name %>
    </div>
    <div class="well">
      <strong>Description:</strong>
      <%= @organization.description %>
    </div>
    <div class="well">
      <strong>Url:</strong>
      <a target ="blank" href=<%= @organization.url %>><%= @organization.url %></a> 
    </div>
    <% if (current_user.role === "admin") %>
      <%= link_to 'Edit', edit_organization_path(@organization) %> |
    <%end%>
    <%= link_to 'Back', organizations_path %>
  </div>
</div>